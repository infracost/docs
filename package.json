{
  "name": "infracost-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve",
    "svgo": "svgo ./static/**/*.svg",
    "lint": "eslint --ext .ts,.js,.tsx,.jsx --ignore-path .eslintignore --ignore-path .gitignore . --max-warnings=0",
    "lintfix": "eslint --fix --ext .ts,.js,.tsx,.jsx --ignore-path .eslintignore --ignore-path .gitignore .",
    "format": "prettier --write 'src/**/*.{js,ts,jsx,tsx}' 'docs/**/*.{md,mdx}'",
    "format:check": "prettier --check 'src/**/*.{js,ts,jsx,tsx}' 'docs/**/*.{md,mdx}'"
  },
  "dependencies": {
    "@docusaurus/core": "^3.4.0",
    "@docusaurus/mdx-loader": "^3.4.0",
    "@docusaurus/plugin-client-redirects": "^3.4.0",
    "@docusaurus/plugin-google-gtag": "^3.4.0",
    "@docusaurus/plugin-google-tag-manager": "^3.4.0",
    "@docusaurus/preset-classic": "^3.4.0",
    "@docusaurus/theme-classic": "^3.4.0",
    "@docusaurus/theme-common": "^3.4.0",
    "@headlessui/react": "^2.1.2",
    "@mdx-js/react": "^3.0.1",
    "@react-spring/parallax": "^9.7.4",
    "@react-spring/web": "^9.7.4",
    "clsx": "^2.1.1",
    "docusaurus-plugin-plausible": "0.0.5",
    "dotenv": "^16.4.5",
    "fuse.js": "^7.0.0",
    "prettier": "^3.3.3",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-helmet-async": "^2.0.5",
    "react-markdown": "^9.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "svgo": "^3.3.2"
  }
}

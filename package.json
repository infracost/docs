    "@docusaurus/theme-classic": "2.2.0",
    "@docusaurus/theme-common": "2.2.0",
    "@headlessui/react": "^1.7.16",
    "@mdx-js/react": "^1.6.17",
    "@react-spring/parallax": "^9.7.3",
